#!/usr/bin/python
# -*- coding:utf-8 -*-

#plot a sine wave from 0 to 4pi

import math
from matplotlib import pylab

#initialize the two lists and the counting variable num.Note is a float
y_values = []
x_values = []
num = 0.0

#collect both num and the sine of num in a list
while num < math.pi * 4:
    y_values.append(math.sin(num))
    x_values.append(num)
    num += 0.1

#plot the x and y values sa red circles
pylab.plot(x_values,y_values,'ro')
pylab.show()
